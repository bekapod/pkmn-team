{
  "parser": "babel-eslint",
  "extends": ["react-app", "airbnb", "plugin:prettier/recommended"],
  "plugins": ["flowtype"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0
  }
}
